{
	"name": "Bremod",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"android": "react-native run-android",
		"ios": "react-native run-ios --simulator=\"iPhone 15 Pro Max\"",
		"log-android": "react-native log-android",
		"android:release": "react-native run-android --mode=release",
		"lint": "eslint .",
		"start": "react-native start --reset-cache",
		"test": "jest",
		"gradlew-clean": "cd android && ./gradlew clean && cd ..",
		"pod-reinstall": "cd ios && pod deintegrate && pod cache clean --all && rm -rf Podfile.lock && pod install && cd ..",
		"android-apk": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
		"android-aab": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd .. ",
		"clean": "rm -rf ~/Library/Developer/Xcode/DerivedData && cd android && ./gradlew clean && cd .. && yarn cache clean --f && watchman watch-del-all && watchman shutdown-server"
	},
	"dependencies": {
		"@react-native-async-storage/async-storage": "^1.23.1",
		"@react-navigation/native": "^6.1.10",
		"@react-navigation/native-stack": "^6.9.18",
		"@reduxjs/toolkit": "^2.2.3",
		"axios": "^1.6.7",
		"buffer": "^6.0.3",
		"react": "18.2.0",
		"react-native": "0.73.4",
		"react-native-fs": "^2.20.0",
		"react-native-modal": "^13.0.1",
		"react-native-read-more-text": "^1.1.2",
		"react-native-responsive-fontsize": "^0.5.1",
		"react-native-safe-area-context": "^4.9.0",
		"react-native-screens": "^3.29.0",
		"react-native-select-dropdown": "^4.0.1",
		"react-native-size-matters": "^0.4.2",
		"react-native-splash-screen": "^3.3.0",
		"react-native-text-input-interactive": "^0.1.5",
		"react-native-text-ticker": "^1.14.0",
		"react-native-toast-message": "^2.2.0",
		"react-native-vector-icons": "^10.1.0",
		"react-redux": "^9.1.1",
		"redux-logger": "^3.0.6",
		"redux-persist": "^6.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.20.0",
		"@babel/preset-env": "^7.20.0",
		"@babel/runtime": "^7.20.0",
		"@react-native/babel-preset": "0.73.21",
		"@react-native/eslint-config": "0.73.2",
		"@react-native/metro-config": "0.73.5",
		"@react-native/typescript-config": "0.73.1",
		"@types/react": "^18.2.6",
		"@types/react-test-renderer": "^18.0.0",
		"babel-jest": "^29.6.3",
		"eslint": "^8.19.0",
		"jest": "^29.6.3",
		"prettier": "2.8.8",
		"react-test-renderer": "18.2.0",
		"typescript": "5.0.4"
	},
	"engines": {
		"node": ">=18"
	}
}
